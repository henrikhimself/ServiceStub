{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Warning",
        "Hj.ServiceStub.Proxy.LoggerMiddleware": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "eventBodyLimitBytes": "10000000"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "ServiceStub"
    }
  },
  "AllowedHosts": "*",
  "Stub": {
    "JsonPath": "d:/Stub/json"
  },
  "ReverseProxy": {
    "Routes": {
      "restfulapi": {
        "ClusterId": "restfulapi",
        "Match": {
          "Path": "singleobject"
        },
        "Order": 100,
        "Transforms": [
          {
            "PathRemovePrefix": "/singleobject"
          }
        ]
      }
    },
    "Clusters": {
      "restfulapi": {
        "Destinations": {
          "destination1": {
            "Address": "https://api.restful-api.dev/objects/7"
          }
        }
      }
    }
  },
  "SelfSignedCertificate": {
    "FilePath": "d:/stub/cert",
    "AlgorithmOid": "1.2.840.10045.2.1",
    "DnsNames": [
      "*.stub.local"
    ]
  },
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "d:/stub/cert/stub-local.crt.pem",
        "KeyPath": "d:/stub/cert/stub-local.key.pem"
      }
    }
  }
}
