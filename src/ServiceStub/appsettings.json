{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Warning",
        "Hj.ServiceStub.Proxy.LoggerMiddleware": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "eventBodyLimitBytes": "10000000"
        }
      }
    ],
    "Properties": {
      "ApplicationName": "ServiceStub"
    }
  },
  "AllowedHosts": "*",
  "Stub": {
    "_JsonPath": "c:/example-path"
  },
  "ReverseProxy": {
    "Routes": {
      "catfact": {
        "ClusterId": "catfact",
        "Match": {
          "Path": "catfact/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/catfact"
          }
        ]
      },
      "restfulapi": {
        "ClusterId": "restfulapi",
        "Match": {
          "Path": "singleobject"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/singleobject"
          }
        ]
      }
    },
    "Clusters": {
      "catfact": {
        "Destinations": {
          "destination1": {
            "Address": "https://catfact.ninja/fact"
          }
        }
      },
      "restfulapi": {
        "Destinations": {
          "destination1": {
            "Address": "https://api.restful-api.dev/objects/7"
          }
        }
      }
    }
  }
}
