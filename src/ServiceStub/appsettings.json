{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "Stub": {
    "_JsonPath": "c:/example-path"
  },
  "ReverseProxy": {
    "Routes": {
      "catfact": {
        "ClusterId": "catfact",
        "Match": {
          "Path": "catfact/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/catfact"
          }
        ]
      },
      "weather": {
        "ClusterId": "weather",
        "Match": {
          "Path": "weather"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/weather"
          }
        ]
      },
      "restfulapi": {
        "ClusterId": "restfulapi",
        "Match": {
          "Path": "singleobject"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/singleobject"
          }
        ]
      }
    },
    "Clusters": {
      "catfact": {
        "Destinations": {
          "destination1": {
            "Address": "https://catfact.ninja/fact"
          }
        }
      },
      "weather": {
        "Destinations": {
          "destination1": {
            "Address": "https://wttr.in/Stockholm?format=2"
          }
        }
      },
      "restfulapi": {
        "Destinations": {
          "destination1": {
            "Address": "https://api.restful-api.dev/objects/7"
          }
        }
      }
    }
  }
}
