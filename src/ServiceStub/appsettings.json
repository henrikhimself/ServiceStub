{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServiceStub": {
    "_JsonPath": "c:/example-path"
  },
  "ReverseProxy": {
    "Routes": {
      "catfact": {
        "ClusterId": "catfact",
        "Match": {
          "Path": "catfact/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/catfact"
          }
        ]
      }
    },
    "Clusters": {
      "catfact": {
        "Destinations": {
          "destination1": {
            "Address": "https://catfact.ninja/fact"
          }
        }
      }
    }
  }
}
